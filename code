#include <iostream>
using namespace std;

int main() {

int i,j,n, elem, a[50],b[50];

cin>>n;

for(i=0; i<n; i++){
	cin>>a[i];
	b[i]=-1;
}


for(i=0; i<n; i++){
	while(a[i+1]<a[i] && i<n){
		i++;
	}
	
if(i!=n-1){
	

	elem=a[i+1];

	j=i;
	while(a[j]<elem && j>=0){
		if(b[j]==-1)
			b[j]=elem;
		j--;

		}

	}




}


for(i=0;i<n;i++)
	cout<<b[i]<<" ";



	// your code goes here
	return 0;
}